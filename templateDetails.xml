<?xml version="1.0" encoding="utf-8"?>
<extension type="template" client="site">
	<name>GPA Style (by UIKit)</name>
	<version>1.0</version>
	<creationDate>April 2021</creationDate>
	<author>Max V. Shumakov</author>
	<authorEmail>admin@joomla.org</authorEmail>
	<copyright>(C) 2017 Open Source Matters, Inc.</copyright>
	<description>TPL_GPASTYLE_XML_DESCRIPTION</description>
	<files>
		<filename>component.php</filename>
		<filename>error.php</filename>
		<filename>index.php</filename>
		<filename>offline.php</filename>
		<filename>templateDetails.xml</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>
		<folder>css</folder>
		<folder>html</folder>
		<folder>images</folder>
		<folder>javascript</folder>
		<folder>language</folder>
		<folder>vendor</folder>
	</files>
	<positions>
		<position>header</position>
			<position>search</position>
			<position>language</position>
		<position>sub-header</position>
		<position>top</position>
			<position>showplace</position>
			<position>actual</position>
		<position>timeline</position>
		<position>main-block</position>
			<position>content</position>
			<position>sidebar</position>
		<position>bottom-a</position>
		<position>bottom-b</position>
		<position>footer</position>
		<position>debug</position>
	</positions>
	<languages folder="language">
		<language tag="en-GB">en-GB/gpa-style.ini</language>
		<language tag="ru-RU">ru-RU/gpa-style.ini</language>
	</languages>
	<config>
	</config>
</extension>
