<?xml version="1.0" encoding="utf-8"?>
<extension type="template" client="site">
	<name>GPA Template(by Bootstrap)</name>
	<version>0.1</version>
	<creationDate>January 2022</creationDate>
	<author>Sevastiyan Nikiforov</author>
	<authorEmail>yalta.kgu.tech@gmail.com</authorEmail>
	<copyright>(C) 2022 The Humanities and Pedagogical Academy</copyright>
	<description>TPL_GPASTYLE_XML_DESCRIPTION</description>
	<files>
		<filename>component.php</filename>
		<filename>error.php</filename>
		<filename>index.php</filename>
		<filename>offline.php</filename>
		<filename>templateDetails.xml</filename>
		<filename>template_preview.png</filename>
		<filename>template_thumbnail.png</filename>
		<folder>css</folder>
		<folder>html</folder>
		<folder>images</folder>
		<folder>javascript</folder>
		<folder>language</folder>
		<folder>vendor</folder>
	</files>

	<positions>
		<position>super-header-left</position>
		<position>super-header-center</position>
		<position>super-header-right</position>

		<position>header</position>

		<position>sub-header-left</position>
		<position>sub-header-right</position>

		<position>actual-left</position>
		<position>actual-right</position>

		<position>footer</position>
		<position>debug</position>
	</positions>

	<languages folder="language">
		<language tag="en-GB">en-GB/gpa-style.ini</language>
		<language tag="ru-RU">ru-RU/gpa-style.ini</language>
	</languages>

	<config>
	</config>
</extension>
